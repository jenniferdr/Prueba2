<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:paddingStart="10dp"
    android:paddingTop="16dp"
    android:paddingEnd="10dp"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="cl.jenni.prueba2.main.DetailBookActivity"
    tools:showIn="@layout/activity_detail_book">

    <TextView
        android:id="@+id/titlte_detailTv"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textSize="10pt"/>

    <TextView
        android:id="@+id/editorial_detailTv"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textSize="8pt"/>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:textSize="8pt"
        android:text="Opiniones: "/>

    <EditText
        android:id="@+id/review_detailEt"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />


</LinearLayout>
